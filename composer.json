    "keywords": ["fresns"],
    "license": "Apache-2.0",
    "homepage": "https://fresns.org",
    "support": {
        "issues": "https://github.com/fresns/website-engine/issues",
        "source": "https://github.com/fresns/website-engine"
    },
    "authors": [
        {
            "name": "Jevan Tang",
            "email": "jevan@fresns.org",
            "homepage": "https://github.com/jevantang",
            "role": "Creator"
        }
